<?xml version="1.0" encoding="UTF-8"?>
<!-- Copyright 2002-2014 MarkLogic Corporation.  All Rights Reserved. -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
  targetNamespace="http://marklogic.com/manage/meters"
  xmlns:meters="http://marklogic.com/manage/meters"
  xmlns:manage="http://marklogic.com/manage"
  xmlns="http://marklogic.com/manage/meters">

  <?xdmp-annotations all?>

  <!--********************************************************************************************************************************** Usage (license) ************************  -->
  
  <xs:element name="architecture" type="xs:string"/>
  <xs:element name="platform" type="xs:string"/>
  <xs:element name="license-key" type="xs:string"/>

  <xs:element name="edition" type="xs:string"/>
  
  <!--************************************************************************************************************************************ Meters XML data layer ***************  -->
  
  <xs:complexType name="meter-document">
    <xs:sequence>
      <xs:element name="period" type="meters:period"/>
      <xs:element name="create-date" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
      <xs:element name="period-start-time" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
      <xs:element ref="meters:cluster-id"/>     
      <xs:element ref="meters:group-id"/>
      <xs:element ref="meters:host-id"/>
      <xs:element ref="meters:cluster-name"/>
      <xs:element ref="meters:group-name"/>
      <xs:element ref="meters:host-name"/>
    </xs:sequence>
  </xs:complexType>
  
  <xs:element name="period" type="meters:period"/>
  
  <xs:simpleType name="period">
    <xs:restriction base="xs:string">
      <xs:enumeration value="raw"/>
      <xs:enumeration value="hour"/>
      <xs:enumeration value="day"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="meter-aggregate">
    <xs:simpleContent>
      <xs:extension base="xs:float">
        <xs:attribute name="id" type="xs:int"/>
        <xs:attribute name="count" type="xs:unsignedLong"/>
        <xs:attribute name="max" type="xs:float"/>
        <xs:attribute name="min" type="xs:float"/>
        <xs:attribute name="sum" type="xs:float"/>
        <xs:attribute name="sum-squares" type="xs:float"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:element name="cluster-id" type="xs:unsignedLong">
    <xs:annotation>
      <xs:documentation>
        A cluster unique ID (64-bit value).
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation> 
  </xs:element>
  
  <xs:element name="group-id" type="xs:unsignedLong">
    <xs:annotation>
      <xs:documentation>
        A group unique id (64-bit value).
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>  
  </xs:element>

  <xs:element name="host-id" type="xs:unsignedLong">
    <xs:annotation>
      <xs:documentation>
        A host unique id (64-bit value).
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>  
  </xs:element>

  <xs:element name="forest-id" type="xs:unsignedLong">
    <xs:annotation>
      <xs:documentation>
        A forest unique id (64-bit value).
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>   
  </xs:element>

  <xs:element name="start-time" type="xs:dateTime">
    <xs:annotation>
      <xs:documentation>
        start time
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units>dt</manage:no-units>
      </xs:appinfo>
    </xs:annotation>     
  </xs:element>
 
  <xs:element name="end-time" type="xs:dateTime">
    <xs:annotation>
      <xs:documentation>
        end time
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units>dt</manage:no-units>
      </xs:appinfo>
    </xs:annotation>     
  </xs:element>
 
  <xs:element name="cluster-name" type="xs:string">
    <xs:annotation>
      <xs:documentation>
        The cluster name.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>     
  </xs:element>
  
  <xs:element name="group-name" type="xs:string">
    <xs:annotation>
      <xs:documentation>
        The group name.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>     
  </xs:element>
  
<!--********************************************************************************************************************************** Forest Status ********  -->
  <xs:element name="forest-statuses">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="meters:meter-document">
          <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="meters:forest-status"/>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="forest-status">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="meters:start-time"/>
        <xs:element ref="meters:end-time"/>        
        <xs:element ref="meters:forest-id"/>
        <xs:element ref="meters:forest-name"/>
        <xs:element ref="meters:is-active" minOccurs="0" maxOccurs="1"/>        
        <xs:element ref="meters:nonblocking-timestamp" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="meters:journals-size"/>
        <xs:element ref="meters:large-data-size"/>
        <xs:element ref="meters:data-size"/>
        <xs:element ref="meters:fast-data-size"/>
        
        <xs:element ref="meters:orphaned-binaries"/>
        <xs:element ref="meters:device-space"/>
        <xs:element ref="meters:device-fs-id"/>
        <xs:element ref="meters:large-device-space"/>
        <xs:element ref="meters:large-device-fs-id"/>
        <xs:element ref="meters:fast-device-space"/>
        <xs:element ref="meters:fast-device-fs-id"/>
        <xs:element ref="meters:query-read-bytes"/>
        <xs:element ref="meters:query-read-time"/>
        <xs:element ref="meters:query-read-rate"/>
        <xs:element ref="meters:query-read-load"/>
        <xs:element ref="meters:journal-write-bytes"/>
        <xs:element ref="meters:journal-write-time"/>
        <xs:element ref="meters:journal-write-rate"/>
        <xs:element ref="meters:journal-write-load"/>
        <xs:element ref="meters:save-write-bytes"/>
        <xs:element ref="meters:save-write-time"/>
        <xs:element ref="meters:save-write-rate"/>
        <xs:element ref="meters:save-write-load"/>
        <xs:element ref="meters:merge-read-bytes"/>
        <xs:element ref="meters:merge-read-time"/>
        <xs:element ref="meters:merge-read-rate"/>
        <xs:element ref="meters:merge-read-load"/>
        <xs:element ref="meters:merge-write-bytes"/>
        <xs:element ref="meters:merge-write-time"/>
        <xs:element ref="meters:merge-write-rate"/>
        <xs:element ref="meters:merge-write-load"/>
        <xs:element ref="meters:backup-read-bytes"/>
        <xs:element ref="meters:backup-read-time"/>
        <xs:element ref="meters:backup-read-rate"/>
        <xs:element ref="meters:backup-read-load"/>
        <xs:element ref="meters:backup-write-bytes"/>
        <xs:element ref="meters:backup-write-time"/>
        <xs:element ref="meters:backup-write-rate"/>
        <xs:element ref="meters:backup-write-load"/>
        <xs:element ref="meters:restore-read-bytes"/>
        <xs:element ref="meters:restore-read-time"/>
        <xs:element ref="meters:restore-read-rate"/>
        <xs:element ref="meters:restore-read-load"/>
        <xs:element ref="meters:restore-write-bytes"/>
        <xs:element ref="meters:restore-write-time"/>
        <xs:element ref="meters:restore-write-rate"/>
        <xs:element ref="meters:restore-write-load"/>
        <xs:element ref="meters:large-read-bytes"/>
        <xs:element ref="meters:large-read-time"/>
        <xs:element ref="meters:large-read-rate"/>
        <xs:element ref="meters:large-read-load"/>
        <xs:element ref="meters:large-write-bytes"/>
        <xs:element ref="meters:large-write-time"/>
        <xs:element ref="meters:large-write-rate"/>
        <xs:element ref="meters:large-write-load"/>
        <xs:element ref="meters:database-replication-receive-bytes"/>
        <xs:element ref="meters:database-replication-receive-time"/>
        <xs:element ref="meters:database-replication-receive-rate"/>
        <xs:element ref="meters:database-replication-receive-load"/>
        <xs:element ref="meters:database-replication-send-bytes"/>
        <xs:element ref="meters:database-replication-send-time"/>
        <xs:element ref="meters:database-replication-send-rate"/>
        <xs:element ref="meters:database-replication-send-load"/>
        <xs:element ref="meters:large-binary-cache-hits"/>
        <xs:element ref="meters:large-binary-cache-misses"/>
        <xs:element ref="meters:large-binary-cache-hit-rate"/>
        <xs:element ref="meters:large-binary-cache-miss-rate"/>
        <xs:element ref="meters:active-fragment-count"/>
        <xs:element ref="meters:nascent-fragment-count"/>
        <xs:element ref="meters:deleted-fragment-count"/>
        <xs:element ref="meters:read-lock-count" />
        <xs:element ref="meters:read-lock-rate" />
        <xs:element ref="meters:read-lock-wait-load" />
        <xs:element ref="meters:read-lock-hold-load" />
        <xs:element ref="meters:write-lock-count" />
        <xs:element ref="meters:write-lock-rate" />
        <xs:element ref="meters:write-lock-wait-load" />
        <xs:element ref="meters:write-lock-hold-load" />
        <xs:element ref="meters:deadlock-count" />
        <xs:element ref="meters:deadlock-rate" />
        <xs:element ref="meters:deadlock-wait-load" />
        <xs:element ref="meters:deadlock-hold-load" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="forest-name" type="xs:string">
    <xs:annotation>
      <xs:documentation>
        The forest name.       
      </xs:documentation>
    </xs:annotation>    
  </xs:element>
  
  <xs:element name="is-active" type="xs:boolean">
    <xs:annotation>
      <xs:documentation>
        Forest status.        
      </xs:documentation>
      <xs:appinfo>
        <manage:units>boolean</manage:units>
      </xs:appinfo>
    </xs:annotation>        
  </xs:element>
  
  <xs:element name="nonblocking-timestamp" type="xs:dateTime">
    <xs:annotation>
      <xs:documentation>
        The most current timestamp for which a query will execute without
        waiting for transactions to settle.
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units>dt</manage:no-units>        
      </xs:appinfo>
    </xs:annotation>    
  </xs:element>
  
  <xs:element name="journals-size" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The amount of space the journals take up on disk.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>MB</manage:units>
      </xs:appinfo>
    </xs:annotation>   
  </xs:element>
  
  <xs:element name="orphaned-binaries" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The count of orphaned large binaries.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>count</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="device-space" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        A number of megabytes.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>MB</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="device-fs-id" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        A filesystem ID.
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>                
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="large-device-space" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The amount of space left on large forest device.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>MB</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="large-device-fs-id" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        A filesystem ID.
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>     
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="fast-device-space" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        A number of megabytes.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>MB</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="fast-device-fs-id" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        A filesystem ID.
      </xs:documentation>
      <xs:appinfo>        
        <manage:no-units/>  
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="database-replication-receive-bytes" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The number of bytes received for database replication.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>bytes</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="database-replication-receive-time" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The time (as xs:dayTimeDuration) to receive data for database replication.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>sec</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="database-replication-receive-rate" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The moving average of the rate for receiving data for database replication, in 
        seconds per second.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>MB/sec</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="database-replication-receive-load" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The moving average of the load for receiving data for database replication, in 
        seconds per second.      </xs:documentation>
      <xs:appinfo>
        <manage:units>sec/sec</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
 
  <xs:element name="database-replication-send-bytes" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The number of bytes sent for database replication.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>bytes</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="database-replication-send-time" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The time (as xs:dayTimeDuration) to send data for database replication.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>sec</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="database-replication-send-rate" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The moving average of sending data for database replication, in megabytes 
        per second.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>MB/sec</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="database-replication-send-load" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The moving average of the load for sending data for database replication, in 
        seconds per second.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>sec/sec</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="large-binary-cache-hits" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The number of hits on the large binary cache.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>count</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="large-binary-cache-misses" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The number of misses on the large binary cache.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>count</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="large-binary-cache-hit-rate" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The average number of hits on the large binary cache.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>avg</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="large-binary-cache-miss-rate" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The average number of misses on the large binary cache.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>avg</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element  name="active-fragment-count" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The number of fragments.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>count</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="nascent-fragment-count" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The number of nascent fragments.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>count</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="deleted-fragment-count" type="meters:meter-aggregate">
    <xs:annotation>
      <xs:documentation>
        The number of deleted fragments.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>count</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  
  <!--********************************************************************************************************************************** Stand ********  -->
  
  <xs:element name="normal-disk-size" type="xs:unsignedLong">
    <xs:annotation>
      <xs:documentation>
        Size on disk.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>MB</manage:units>
      </xs:appinfo>
    </xs:annotation> 
  </xs:element>
  
  <xs:element name="fast-disk-size" type="xs:unsignedLong">
    <xs:annotation>
      <xs:documentation>
        Size on fast disk.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>MB</manage:units>
      </xs:appinfo>
    </xs:annotation>   
  </xs:element>

  <xs:element name="memory-size" type="xs:unsignedLong">
    <xs:annotation>
      <xs:documentation>
        The amount of space the stand takes in memory.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>MB</manage:units>
      </xs:appinfo>
    </xs:annotation>  
  </xs:element>
  
  <!--********************************************************************************************************************************** Host Status ********  -->

    <xs:element name="host-statuses">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="meters:meter-document">
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" ref="meters:host-status"/>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="host-status">
    <xs:complexType>
            <xs:sequence>
            <xs:element ref="meters:start-time"/>
            <xs:element ref="meters:end-time"/>
            <xs:element ref="meters:cpus" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:cores" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:core-threads" minOccurs="0" maxOccurs="unbounded"/>      
            <xs:element ref="meters:total-host-count" minOccurs="0" maxOccurs="unbounded"/>  
            <xs:element ref="meters:total-cpu-stat-user" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:total-cpu-stat-nice" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:total-cpu-stat-system" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:total-cpu-stat-idle" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:total-cpu-stat-iowait" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:total-cpu-stat-irq" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:total-cpu-stat-softirq" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:total-cpu-stat-steal" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:total-cpu-stat-guest" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:total-cpu-stat-guest-nice" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:cpu-stat-user" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:cpu-stat-nice" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:cpu-stat-system" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:cpu-stat-idle" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:cpu-stat-iowait" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:cpu-stat-irq" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:cpu-stat-softirq" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:cpu-stat-steal" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:cpu-stat-guest" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:cpu-stat-guest-nice" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="meters:memory-process-size" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="meters:memory-process-rss" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="meters:memory-process-anon" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="meters:memory-process-rss-hwm" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="meters:memory-process-swap-rate" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="meters:memory-system-pagein-rate" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="meters:memory-system-pageout-rate" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="meters:memory-system-swapin-rate" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="meters:memory-system-swapout-rate" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="meters:host-size"/>
            <xs:element ref="meters:host-large-data-size"/>
            <xs:element ref="meters:log-device-space"/>
            <xs:element ref="meters:data-dir-space"/>
            <xs:element ref="meters:query-read-bytes"/>
            <xs:element ref="meters:query-read-time"/>
            <xs:element ref="meters:query-read-rate"/>
            <xs:element ref="meters:query-read-load"/>
            <xs:element ref="meters:journal-write-bytes"/>
            <xs:element ref="meters:journal-write-time"/>
            <xs:element ref="meters:journal-write-rate"/>
            <xs:element ref="meters:journal-write-load"/>
