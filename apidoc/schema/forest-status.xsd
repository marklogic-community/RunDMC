<xs:schema targetNamespace="http://marklogic.com/xdmp/status/forest"
 xsi:schemaLocation="http://www.w3.org/2001/XMLSchema XMLSchema.xsd"
 xmlns="http://marklogic.com/xdmp/status/forest"
 xmlns:admin="http://marklogic.com/xdmp/admin"
 xmlns:manage="http://marklogic.com/xdmp/manage"
 xmlns:xs="http://www.w3.org/2001/XMLSchema"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:xhtml="http://www.w3.org/1999/xhtml"
 xmlns:xa="http://marklogic.com/xdmp/xa"
 xmlns:cts="http://marklogic.com/cts"
 >

  <?xdmp-annotations?>

  <xs:import namespace="http://marklogic.com/xdmp/xa" schemaLocation="xa.xsd">
    <xs:annotation>
      <xs:documentation>
        For the XA xid element.
      </xs:documentation>
    </xs:annotation>
  </xs:import>

  <xs:import namespace="http://marklogic.com/cts"
                 schemaLocation="cts.xsd"/>

  <xs:simpleType name="forest-id">
    <xs:annotation>
      <xs:documentation>
        A forest unique ID (64-bit value).
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong">
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="forest-id" type="forest-id">
    <xs:annotation>
      <xs:documentation>
  	The unique key of the forest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="forest-name">
    <xs:annotation>
      <xs:documentation>
        The forest name.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:minLength value="1"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="forest-name" type="forest-name">
    <xs:annotation>
      <xs:documentation>
        The forest name.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="merge-id">
    <xs:annotation>
      <xs:documentation>
        A key for a merge record.
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong">
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="merge-id" type="merge-id">
    <xs:annotation>
      <xs:documentation>
        A unique key for a merge.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="boolean">
    <xs:annotation>
      <xs:documentation>
        true or false
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:boolean"/>
  </xs:simpleType>

  <xs:element name="canceled" type="boolean">
    <xs:annotation>
      <xs:documentation>
        Is a merge canceled or not.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="enabled" type="boolean">
    <xs:annotation>
      <xs:documentation>
        Is the forest enabled or not.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="host-id">
    <xs:annotation>
      <xs:documentation>
        A host unique ID (64-bit value).
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong">
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="host-id" type="host-id">
    <xs:annotation>
      <xs:documentation>
  	The unique key of the host that mounts the forest
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="state">
    <xs:annotation>
      <xs:documentation>
        A forest state.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="mount error"/>
      <xs:enumeration value="error"/>
      <xs:enumeration value="unmounted"/>
      <xs:enumeration value="mounted"/>
      <xs:enumeration value="converting"/>
      <xs:enumeration value="recovering"/>
      <xs:enumeration value="wait replication"/>
      <xs:enumeration value="async replicating"/>
      <xs:enumeration value="sync replicating"/>
      <xs:enumeration value="open"/>
      <xs:enumeration value="start closing"/>
      <xs:enumeration value="middle closing"/>
      <xs:enumeration value="finish closing"/>
      <xs:enumeration value="syncing replica"/>
      <xs:enumeration value="open replica"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="state" type="state">
    <xs:annotation>
      <xs:documentation>
  	The state of the forest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="updates-allowed">
    <xs:annotation>
      <xs:documentation>
        The types of updates allowed on a forest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="all"/>
      <xs:enumeration value="delete-only"/>
      <xs:enumeration value="read-only"/>
      <xs:enumeration value="flash-backup"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="updates-allowed" type="updates-allowed">
    <xs:annotation>
      <xs:documentation>
  	The types of updates allowed on the forest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="rebalancer-enable" type="boolean">
    <xs:annotation>
      <xs:documentation>
        Is the rebalancer on this forest enabled or not.
      </xs:documentation>
      <xs:appinfo>  
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:simpleType name="destination">
    <xs:annotation>
      <xs:documentation>
        A destination of an operation.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="rebalance-destination" type="destination">
    <xs:annotation>
      <xs:documentation>
        The destination of the rebalancing operation.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="lower-bound" type="xs:anySimpleType">
    <xs:annotation>
      <xs:documentation>
        The lower bound of the range on the forest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="upper-bound" type="xs:anySimpleType">
    <xs:annotation>
      <xs:documentation>
        The upper bound of the range on the forest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="stand-kind">
    <xs:annotation>
      <xs:documentation>
        A kind of stand.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="Active"/>
      <xs:enumeration value="Deleting"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="stand-kind" type="stand-kind">
    <xs:annotation>
      <xs:documentation>
  	The kind of stand.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="stands">
    <xs:annotation>
      <xs:documentation>
        A set of forest stands
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="stand"/>
    </xs:choice>
  </xs:complexType>

  <xs:element name="stands" type="stands">
    <xs:annotation>
      <xs:documentation>
  	The stands in the forest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="input-stands">
    <xs:annotation>
      <xs:documentation>
        A set of stands used as input.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="stand-id"/>
    </xs:choice>
  </xs:complexType>

  <xs:element name="input-stands" type="input-stands">
    <xs:annotation>
      <xs:documentation>
  	The input stands of a merge.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="stand-id">
    <xs:annotation>
      <xs:documentation>
        A stand unique ID (64-bit value).
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong">
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="stand-id" type="stand-id">
    <xs:annotation>
      <xs:documentation>
  	The unique key of a stand.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="output-stand" type="stand-id">
    <xs:annotation>
      <xs:documentation>
  	The ID of a stand resulting from a merge.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="path">
    <xs:annotation>
      <xs:documentation>
        A file system path.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="path" type="path">
    <xs:annotation>
      <xs:documentation>
  	The path of the stand.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:simpleType name="data-dir">
    <xs:annotation>
      <xs:documentation>
        The data directory.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="data-dir" type="data-dir">
    <xs:annotation>
      <xs:documentation>
        The data directory.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="large-data-dir" type="data-dir">
    <xs:annotation>
      <xs:documentation>
        The large data directory.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="fast-data-dir" type="data-dir">
    <xs:annotation>
      <xs:documentation>
        The fast data directory.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="obsolete" type="boolean">
    <xs:annotation>
      <xs:documentation>
  	Is this stand obsolete (true for obsolete)
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="label-version">
    <xs:annotation>
      <xs:documentation>
        A Label version number.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="label-version" type="label-version">
    <xs:annotation>
      <xs:documentation>
  	The version number of the label file.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="count">
    <xs:annotation>
      <xs:documentation>
        A count.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>quantity</manage:units>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="average">
    <xs:annotation>
      <xs:documentation>
        An average.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:float">
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="list-cache-hits" type="count">
    <xs:annotation>
      <xs:documentation>
  	The number of hits on the list cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="list-cache-misses" type="count">
    <xs:annotation>
      <xs:documentation>
  	The number of misses on the list cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="compressed-tree-cache-hits" type="count">
    <xs:annotation>
      <xs:documentation>
  	The number of hits on the compressed cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="compressed-tree-cache-misses" type="count">
    <xs:annotation>
      <xs:documentation>
  	The number of misses on the compressed cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="triple-cache-hits" type="count">
    <xs:annotation>
      <xs:documentation>
  	The number of hits on the list cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="triple-cache-misses" type="count">
    <xs:annotation>
      <xs:documentation>
  	The number of misses on the list cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="triple-value-cache-hits" type="count">
    <xs:annotation>
      <xs:documentation>
  	The number of hits on the list cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="triple-value-cache-misses" type="count">
    <xs:annotation>
      <xs:documentation>
  	The number of misses on the list cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="large-binary-cache-hits" type="count">
     <xs:annotation>
       <xs:documentation>
         The number of hits on the large binary cache.
       </xs:documentation>
       <xs:appinfo>
       </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="large-binary-cache-misses" type="count">
    <xs:annotation>
      <xs:documentation>
        The number of misses on the large binary cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
   </xs:annotation>
  </xs:element>

  <xs:element name="list-cache-hit-rate" type="average">
    <xs:annotation>
      <xs:documentation>
  	The average number of hits on the list cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="list-cache-miss-rate" type="average">
    <xs:annotation>
      <xs:documentation>
  	The average number of misses on the list cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="compressed-tree-cache-hit-rate" type="average">
    <xs:annotation>
      <xs:documentation>
  	The average number of hits on the compressed cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="compressed-tree-cache-miss-rate" type="average">
    <xs:annotation>
      <xs:documentation>
  	The average number of misses on the compressed cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="triple-cache-hit-rate" type="average">
    <xs:annotation>
      <xs:documentation>
  	The average number of hits on the list cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="triple-cache-miss-rate" type="average">
    <xs:annotation>
      <xs:documentation>
  	The average number of misses on the list cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="triple-value-cache-hit-rate" type="average">
    <xs:annotation>
      <xs:documentation>
  	The average number of hits on the list cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="triple-value-cache-miss-rate" type="average">
    <xs:annotation>
      <xs:documentation>
  	The average number of misses on the list cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="large-binary-cache-hit-rate" type="average">
    <xs:annotation>
      <xs:documentation>
        The average number of hits on the large binary cache.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="large-binary-cache-miss-rate" type="average">
     <xs:annotation>
       <xs:documentation>
         The average number of misses on the large binary cache.
       </xs:documentation>
       <xs:appinfo>
       </xs:appinfo>
     </xs:annotation>
  </xs:element>

  <xs:complexType name="stand">
    <xs:annotation>
      <xs:documentation>
        A forest stand.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="stand-id"/>
      <xs:element ref="path"/>
      <xs:element ref="stand-kind"/>
      <xs:element ref="is-fast"/>
      <xs:element ref="label-version"/>
      <xs:element ref="disk-size"/>
      <xs:element ref="memory-size"/>
      <xs:element ref="list-cache-hits"/>
      <xs:element ref="list-cache-misses"/>
      <xs:element ref="list-cache-hit-rate"/>
      <xs:element ref="list-cache-miss-rate"/>
      <xs:element ref="compressed-tree-cache-hits"/>
      <xs:element ref="compressed-tree-cache-misses"/>
      <xs:element ref="compressed-tree-cache-hit-rate"/>
      <xs:element ref="compressed-tree-cache-miss-rate"/>
      <xs:element ref="triple-cache-hits"/>
      <xs:element ref="triple-cache-misses"/>
      <xs:element ref="triple-cache-hit-rate"/>
      <xs:element ref="triple-cache-miss-rate"/>
      <xs:element ref="triple-value-cache-hits"/>
      <xs:element ref="triple-value-cache-misses"/>
      <xs:element ref="triple-value-cache-hit-rate"/>
      <xs:element ref="triple-value-cache-miss-rate"/>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="stand" type="stand">
    <xs:annotation>
      <xs:documentation>
  	The stand of a forest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="backups">
    <xs:annotation>
      <xs:documentation>
        A set of backups on a forest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="backup"/>
    </xs:choice>
  </xs:complexType>

  <xs:element name="backups" type="backups">
    <xs:annotation>
      <xs:documentation>
  	The backups in progress on the forest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="merges">
    <xs:annotation>
      <xs:documentation>
        A set of merges on a forest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="merge"/>
    </xs:choice>
  </xs:complexType>

  <xs:element name="merges" type="merges">
    <xs:annotation>
      <xs:documentation>
  	The merges in progress on the forest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="start-time" type="timestamp">
    <xs:annotation>
      <xs:documentation>
  	The time a merge started.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:simpleType name="rebalancing">
    <xs:annotation>
      <xs:documentation>
        Is this forest rebalancing or not
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:boolean">
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="rebalancing" type="rebalancing">
    <xs:annotation>
      <xs:documentation>
        Is this forest rebalancing.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="reindexing">
    <xs:annotation>
      <xs:documentation>
        Is this forest reindexing or not
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:boolean">
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="reindexing" type="reindexing">
    <xs:annotation>
      <xs:documentation>
  	Is this forest reindexing.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:simpleType name="journal-archive-lag-limit">
    <xs:annotation>
      <xs:documentation>
        Number of seconds the archived journal can lag the active journal.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="journal-archive-lag-limit" type="journal-archive-lag-limit">
    <xs:annotation>
      <xs:documentation>
	Number of seconds the archived journal can lag the active journal.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="journal-archiving" type="boolean">
    <xs:annotation>
      <xs:documentation>
  	Is journal archiving enabled for the backup?
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="is-fast" type="boolean">
