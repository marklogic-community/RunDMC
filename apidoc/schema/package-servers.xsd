<xs:schema xsi:schemaLocation="http://www.w3.org/2001/XMLSchema XMLSchema.xsd" elementFormDefault="qualified" targetNamespace="http://marklogic.com/manage/package/servers" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://marklogic.com/manage/package/servers" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <xs:simpleType name="address">
    <xs:annotation>
      <xs:documentation>
        A numeric internet address.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:pattern value="[0-9]+\.[0-9]+\.[0-9]+\.[0-9]+"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="port">
    <xs:annotation>
      <xs:documentation>
        An integer internet port number, min 1, max 65534.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1" fixed="false"/>
      <xs:maxInclusive value="65534" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="backlog">
    <xs:annotation>
      <xs:documentation>
        An integer number of connections, min 1, max 4096.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1" fixed="false"/>
      <xs:maxInclusive value="4096" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="threadCount">
    <xs:annotation>
      <xs:documentation>
        An integer number of threads, min 1, max 256.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1" fixed="false"/>
      <xs:maxInclusive value="256" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="directory-pathname">
    <xs:annotation>
      <xs:documentation>
        It can be absolute or relative.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="request-limit">
    <xs:annotation>
      <xs:documentation>
        An integer number of seconds, min 0, max 4294967295.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="0" fixed="false"/>
      <xs:maxInclusive value="4294967295" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="timeout">
    <xs:annotation>
      <xs:documentation>
        An integer number of seconds, min 0, max 4294967295.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="0" fixed="false"/>
      <xs:maxInclusive value="4294967295" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="multi-version-concurrency-control">
    <xs:annotation>
      <xs:documentation>
        contemporaneous or nonblocking
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="contemporaneous"/>
      <xs:enumeration value="nonblocking"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="distribute-timestamps">
    <xs:annotation>
      <xs:documentation>
        fast or strict
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="fast"/>
      <xs:enumeration value="strict"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="authentication">
    <xs:annotation>
      <xs:documentation>
        The authentication mode for the server.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="application-level"/>
      <xs:enumeration value="basic"/>
      <xs:enumeration value="digest"/>
      <xs:enumeration value="digestbasic"/>
      <xs:enumeration value="kerberos-ticket"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="external-security">
    <xs:annotation>
      <xs:documentation>
        External security.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:simpleType name="ssl-certificate-template-id">
    <xs:annotation>
      <xs:documentation>
        The id of the certificate template in the security database.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:simpleType name="ssl-hostname">
    <xs:annotation>
      <xs:documentation>
        A hostname.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="0" fixed="false"/>
      <xs:maxLength value="64" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ssl-ciphers-string">
    <xs:annotation>
      <xs:documentation>
        The id of the certificate template in the security database.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="sgml-entity-mapping">
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="none"/>
      <xs:enumeration value="normal"/>
      <xs:enumeration value="math"/>
      <xs:enumeration value="pub"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="output-encoding">
    <xs:annotation>
      <xs:documentation>
        An application server's default output encoding.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="UTF-8"/>
      <xs:enumeration value="ASCII"/>
      <xs:enumeration value="ISO-8859-1"/>
      <xs:enumeration value="ISO-8859-5"/>
      <xs:enumeration value="ISO-8859-6"/>
      <xs:enumeration value="ISO-2022-KR"/>
      <xs:enumeration value="ISO-2022-JP"/>
      <xs:enumeration value="EUC-CN"/>
      <xs:enumeration value="EUC-KR"/>
      <xs:enumeration value="EUC-JP"/>
      <xs:enumeration value="CP932"/>
      <xs:enumeration value="CP936"/>
      <xs:enumeration value="CP949"/>
      <xs:enumeration value="CP950"/>
      <xs:enumeration value="CP1252"/>
      <xs:enumeration value="CP1256"/>
      <xs:enumeration value="KOI8-R"/>
      <xs:enumeration value="GB12052"/>
      <xs:enumeration value="GB18030"/>
      <xs:enumeration value="GB2312"/>
      <xs:enumeration value="HZ-GB-2312"/>
      <xs:enumeration value="BIG5"/>
      <xs:enumeration value="BIG5-HKSCS"/>
      <xs:enumeration value="Shift_JIS"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="default-xquery-version">
    <xs:annotation>
      <xs:documentation>
        An application server's default XQuery language version.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="1.0-ml"/>
      <xs:enumeration value="0.9-ml"/>
      <xs:enumeration value="1.0"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="boolean">
    <xs:annotation>
      <xs:documentation>
        true or false
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:boolean"/>
  </xs:simpleType>
  <xs:simpleType name="pre-commit-trigger-limit">
    <xs:annotation>
      <xs:documentation>
        A maximum number of triggers a single statement can invoke.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="trigger-depth">
    <xs:annotation>
      <xs:documentation>
        An integer number of trigger invocation.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="collation">
    <xs:annotation>
      <xs:documentation>
        A collation.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:anyURI"/>
  </xs:simpleType>
  <xs:simpleType name="error-handler">
    <xs:annotation>
      <xs:documentation>
        The error handler.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:anyURI"/>
  </xs:simpleType>
  <xs:simpleType name="url-rewriter">
    <xs:annotation>
      <xs:documentation>
        The URL rewriter.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:anyURI"/>
  </xs:simpleType>
  <xs:simpleType name="output-method">
    <xs:annotation>
      <xs:documentation>
        Per 'method' attribute from xsl:output, one of: "xml", "xhtml", "html", or "text".
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="default"/>
      <xs:enumeration value="xml"/>
      <xs:enumeration value="xhtml"/>
      <xs:enumeration value="html"/>
      <xs:enumeration value="text"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="yes-or-no">
    <xs:annotation>
      <xs:documentation>
       Nullable Boolean type accepting values "yes", "no", and "default".
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="yes"/>
      <xs:enumeration value="no"/>
      <xs:enumeration value="default"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="output-cdata-section-namespace-uri">
    <xs:annotation>
      <xs:documentation>
        A namespace URI string.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:anyURI"/>
  </xs:simpleType>
  <xs:simpleType name="output-cdata-section-localname">
    <xs:annotation>
      <xs:documentation>
       A list of element localname strings
      </xs:documentation>
    </xs:annotation>
    <xs:list itemType="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="output-doctype-public">
    <xs:annotation>
      <xs:documentation>
        A public DOCTYPE string.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:anyURI"/>
  </xs:simpleType>
  <xs:simpleType name="output-doctype-system">
    <xs:annotation>
      <xs:documentation>
        A system DOCTYPE string.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:anyURI"/>
  </xs:simpleType>
  <xs:simpleType name="output-media-type">
    <xs:annotation>
      <xs:documentation>
        A MIME content type name.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:pattern value="([0-9A-Za-z.+\-]+/[0-9A-Za-z.+\-]+)?"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="output-normalization-form">
    <xs:annotation>
      <xs:documentation>
        A normalization token.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="none"/>
      <xs:enumeration value="NFC"/>
      <xs:enumeration value="NFD"/>
      <xs:enumeration value="NFKD"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="yes-or-no-or-omit">
    <xs:annotation>
      <xs:documentation>
       Nullable Boolean type, taking values "yes", "no", and "omit".
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="omit"/>
      <xs:enumeration value="yes"/>
      <xs:enumeration value="no"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="output-version">
    <xs:annotation>
      <xs:documentation>
        A version string.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token"/>
  </xs:simpleType>
  <xs:simpleType name="ssl-certificate-authority-id">
    <xs:annotation>
      <xs:documentation>
        An SSL certificate authority ID.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:simpleType name="namespace-uri">
    <xs:annotation>
      <xs:documentation>
        A namespace URI.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:anyURI"/>
  </xs:simpleType>
  <xs:simpleType name="schema-location">
    <xs:annotation>
      <xs:documentation>
        A URI used to find a schema.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:anyURI"/>
  </xs:simpleType>
  <xs:simpleType name="prefix">
    <xs:annotation>
      <xs:documentation>
        A QName prefix.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:NCName"/>
  </xs:simpleType>
  <xs:simpleType name="location">
    <xs:annotation>
      <xs:documentation>
        A module location.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:anyURI"/>
  </xs:simpleType>
  <xs:simpleType name="role-id">
    <xs:annotation>
      <xs:documentation>
        Role ids.
      </xs:documentation>
    </xs:annotation>
    <xs:list itemType="xs:NMTOKEN"/>
  </xs:simpleType>
  <xs:simpleType name="user-ids">
    <xs:annotation>
      <xs:documentation>
        User ids.
      </xs:documentation>
    </xs:annotation>
    <xs:list itemType="xs:NMTOKEN"/>
  </xs:simpleType>
  <xs:simpleType name="blackout-type">
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="once"/>
      <xs:enumeration value="recurring"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="start-time">
    <xs:annotation>
      <xs:documentation>
  The starting time (in 24:00 notation).
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:time"/>
  </xs:simpleType>
  <xs:simpleType name="end-time">
    <xs:annotation>
      <xs:documentation>
  The ending time (in 24:00 notation).
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:time"/>
  </xs:simpleType>
  <xs:simpleType name="start-date">
    <xs:annotation>
      <xs:documentation>
        The starting date (in MM/DD/YYYY notation)
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:date"/>
  </xs:simpleType>
  <xs:simpleType name="end-date">
    <xs:annotation>
      <xs:documentation>
        The ending date (in MM/DD/YYYY notation)
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:date"/>
  </xs:simpleType>
  <xs:simpleType name="duration">
    <xs:annotation>
      <xs:documentation>
        The duration, specifed as hours:minutes (i.e. 2 hours 5 minutes is 2:05)
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:duration"/>
  </xs:simpleType>
  <xs:simpleType name="day">
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="monday"/>
      <xs:enumeration value="tuesday"/>
      <xs:enumeration value="wednesday"/>
      <xs:enumeration value="thursday"/>
      <xs:enumeration value="friday"/>
      <xs:enumeration value="saturday"/>
      <xs:enumeration value="sunday"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="schemas" abstract="false" mixed="false">
    <xs:annotation>
      <xs:documentation>
        The schema binding specifications.
      </xs:documentation>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="schema" minOccurs="1" maxOccurs="1"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="ssl-client-certificate-authorities" abstract="false" mixed="false">
    <xs:annotation>
      <xs:documentation>
        The certificate authorities that may sign a client certificate.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="ssl-client-certificate-authority" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="namespaces" abstract="false" mixed="false">
    <xs:annotation>
      <xs:documentation>
        The namespace binding specifications.
      </xs:documentation>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="namespace" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="using-namespace" minOccurs="1" maxOccurs="1"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="module-locations" abstract="false" mixed="false">
    <xs:annotation>
      <xs:documentation>
        The module location specifications.
      </xs:documentation>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="module-location" minOccurs="1" maxOccurs="1"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="request-blackouts" abstract="false" mixed="false">
    <xs:annotation>
      <xs:documentation>
        Users, roles, orperiods for request blackouts.
      </xs:documentation>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="request-blackout" minOccurs="1" maxOccurs="1"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="schema" abstract="false" mixed="false">
    <xs:annotation>
      <xs:documentation>
        A schema binding specification.
      </xs:documentation>
    </xs:annotation>
    <xs:all minOccurs="1" maxOccurs="1">
      <xs:element ref="namespace-uri" minOccurs="0" maxOccurs="1"/>
      <xs:element ref="schema-location" minOccurs="1" maxOccurs="1"/>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="namespace" abstract="false" mixed="false">
    <xs:annotation>
      <xs:documentation>
        A namespace binding specification.
      </xs:documentation>
    </xs:annotation>
    <xs:all minOccurs="1" maxOccurs="1">
      <xs:element ref="prefix" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="namespace-uri" minOccurs="1" maxOccurs="1"/>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="using-namespace" abstract="false" mixed="false">
    <xs:annotation>
      <xs:documentation>
        A namespace path URI.
      </xs:documentation>
    </xs:annotation>
    <xs:all minOccurs="1" maxOccurs="1">
      <xs:element ref="namespace-uri" minOccurs="1" maxOccurs="1"/>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="module-location" abstract="false" mixed="false">
    <xs:annotation>
      <xs:documentation>
        A module location specification.
      </xs:documentation>
    </xs:annotation>
    <xs:all minOccurs="1" maxOccurs="1">
      <xs:element ref="namespace-uri" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="location" minOccurs="1" maxOccurs="1"/>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="request-blackout" abstract="false" mixed="false">
    <xs:annotation>
      <xs:documentation>
        A merge blackout period.
      </xs:documentation>
    </xs:annotation>
    <xs:all minOccurs="1" maxOccurs="1">
      <xs:element ref="users" minOccurs="0" maxOccurs="1"/>
      <xs:element ref="roles" minOccurs="0" maxOccurs="1"/>
      <xs:element ref="blackout-type" minOccurs="0" maxOccurs="1"/>
      <xs:element ref="days" minOccurs="0" maxOccurs="1"/>
      <xs:element ref="period" minOccurs="1" maxOccurs="1"/>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="period" abstract="false" mixed="false">
    <xs:annotation>
      <xs:documentation>
        A period, with a start and end time/duration.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element ref="start-date" minOccurs="0" maxOccurs="1"/>
      <xs:element ref="start-time" minOccurs="0" maxOccurs="1"/>
      <xs:choice minOccurs="1" maxOccurs="1">
	<xs:sequence minOccurs="1" maxOccurs="1">
	  <xs:element ref="end-date" minOccurs="0" maxOccurs="1"/>
	  <xs:element ref="end-time" minOccurs="0" maxOccurs="1"/>
	</xs:sequence>
	<xs:element ref="duration" minOccurs="0" maxOccurs="1"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="days" abstract="false" mixed="false">
    <xs:annotation>
      <xs:documentation>
        A selection of days
      </xs:documentation>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="7">
      <xs:element ref="day" minOccurs="1" maxOccurs="1"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="package-http-properties">
    <xs:annotation>
      <xs:documentation>The packaged properties of an HTTP server.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" maxOccurs="1" ref="enabled"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="root"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="port"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="webDAV"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="execute"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="display-last-login"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="address"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="backlog"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="threads"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="request-timeout"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="keep-alive-timeout"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="session-timeout"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="max-time-limit"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="default-time-limit"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="static-expires"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="pre-commit-trigger-depth"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="pre-commit-trigger-limit"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="collation"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="authentication"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="internal-security"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="external-security"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="concurrent-request-limit"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="compute-content-length"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="log-errors"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="debug-allow"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="profile-allow"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="default-xquery-version"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="multi-version-concurrency-control"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="distribute-timestamps"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="output-sgml-character-entities"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="output-encoding"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="output-method"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="output-byte-order-mark"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="output-cdata-section-namespace-uri"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="output-cdata-section-localname"/>
      <xs:element minOccurs="0" maxOccurs="1" ref="output-doctype-public"/>
