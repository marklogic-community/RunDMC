<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- Copyright 2002-2014 MarkLogic Corporation.  All Rights Reserved. -->
<xs:schema xmlns="http://marklogic.com/xdmp/status/host" xmlns:admin="http://marklogic.com/xdmp/admin" xmlns:manage="http://marklogic.com/xdmp/manage" xmlns:xa="http://marklogic.com/xdmp/xa" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" elementFormDefault="qualified" targetNamespace="http://marklogic.com/xdmp/status/host" xsi:schemaLocation="http://www.w3.org/2001/XMLSchema XMLSchema.xsd">

  <xs:import namespace="http://marklogic.com/xdmp/xa" schemaLocation="xa.xsd">
    <xs:annotation>
      <xs:documentation>
        For the XA xid element.
      </xs:documentation>
    </xs:annotation>
  </xs:import>
  
  <xs:simpleType name="zone">
    <xs:annotation>
      <xs:documentation>
        Zone the host belongs to.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>
  
  <xs:element name="zone" type="zone">
    <xs:annotation>
      <xs:documentation>
        Zone the host belongs to.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  
  <xs:simpleType name="cpus">
    <xs:annotation>
      <xs:documentation>
        A count of CPUs.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
    </xs:restriction>
  </xs:simpleType>
  
    <xs:element name="cpus" type="cpus">
    <xs:annotation>
      <xs:documentation>
        The number of CPUs.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:complexType name="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        A cpu utilization expressed as a percentage from 0 to 100
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:float">
        <xs:attribute name="id" type="xs:int"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  
  <xs:simpleType name="cores">
    <xs:annotation>
      <xs:documentation>
        A count of cores.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="cores" type="cores">
    <xs:annotation>
      <xs:documentation>
        The number of cores.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="core-threads">
    <xs:annotation>
      <xs:documentation>
        A count of total core threads.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
    </xs:restriction>
  </xs:simpleType>
  
  <xs:element name="core-threads" type="core-threads">
    <xs:annotation>
      <xs:documentation>
        The number of core threads (including intel hyper threads).
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  
  
  
  <xs:element name="total-cpu-stat-user" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for user.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="total-cpu-stat-nice" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for nice.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="total-cpu-stat-system" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for system.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="total-cpu-stat-idle" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for idle.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="total-cpu-stat-iowait" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for iowait.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="total-cpu-stat-irq" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for irq.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="total-cpu-stat-softirq" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for softirq.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="total-cpu-stat-steal" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for steal.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  <xs:element name="total-cpu-stat-guest" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for guest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="total-cpu-stat-guest-nice" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for guest nice.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  
  
  
  
  <xs:element name="cpu-stat-user" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for user.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="cpu-stat-nice" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for nice.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="cpu-stat-system" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for system.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="cpu-stat-idle" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for idle.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="cpu-stat-iowait" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for iowait.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="cpu-stat-irq" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for irq.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="cpu-stat-softirq" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for softirq.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="cpu-stat-steal" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for steal.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  <xs:element name="cpu-stat-guest" type="cpu-stat">
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for guest.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element> 
  
  
  <xs:element name="cpu-stat-guest-nice" type="cpu-stat">
    
    <xs:annotation>
      <xs:documentation>
        Total cpu utilization for guest nice.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>

  </xs:element> 
  
  
  
  
  
  
  <xs:element name="memory-process-rss" type="memory-size">
    <xs:annotation>
      <xs:documentation>
        The number of MB of Process Resident Size (RSS) for the MarkLogic process.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="memory-process-rss-hwm" type="memory-size">
    <xs:annotation>
      <xs:documentation>
        The mazimum (High Water Mark) of Process Resident Size (RSS) in MB for the MarkLogic process.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="memory-process-size" type="memory-size">
    <xs:annotation>
      <xs:documentation>
        The number of MB of total process virtual memory for the MarkLogic process.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="memory-process-anon" type="memory-size">
    <xs:annotation>
      <xs:documentation>
        The number of MB of Process Anonymous Memory for the MarkLogic process.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="memory-process-swap-rate" type="xs:float">
    <xs:annotation>
      <xs:documentation>
        The swap rate for the MarkLogic process in pages/sec.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  
  <xs:element name="memory-system-pagein-rate" type="xs:float">
    <xs:annotation>
      <xs:documentation>
        The page in rate for the system in pages/sec.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  
  <xs:element name="memory-system-pageout-rate" type="xs:float">
    <xs:annotation>
      <xs:documentation>
        The page out rate for the system in pages/sec.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  
  <xs:element name="memory-system-swapin-rate" type="xs:float">
    <xs:annotation>
      <xs:documentation>
        The swap in rate for the system in pages/sec.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="memory-system-swapout-rate" type="xs:float">
    <xs:annotation>
      <xs:documentation>
        The swap out rate for the system in pages/sec.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="memory-size" type="memory-size">
    <xs:annotation>
      <xs:documentation>
        The number of MB of system memory.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:simpleType name="device-space">
    <xs:annotation>
      <xs:documentation>
        An amount (in MB) of space left on a device.
      </xs:documentation>
      <xs:appinfo>
        <manage:units>MB</manage:units>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="host-size" type="device-space">
    <xs:annotation>
      <xs:documentation>
	      The total data size of all forests on the host.  The size is
	      calculated by adding the fast data directory size and the 
	      data directory size for each forest on this host, but
	      it does not include any content stored in the large data 
	      directory.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="host-large-data-size" type="device-space">
    <xs:annotation>
      <xs:documentation>
        The total size of all large binary data on the host.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="log-device-space" type="device-space">
    <xs:annotation>
      <xs:documentation>
        The number of MB left on log device.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="data-dir-space" type="device-space">
    <xs:annotation>
      <xs:documentation>
        The number of MB left on data dir.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="online" type="boolean">
    <xs:annotation>
      <xs:documentation>
        Is this host seen as online? (true for online)
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="secure" type="boolean">
    <xs:annotation>
      <xs:documentation>
        Is communication secured with SSL? For the local host, this is always
        false.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="connect-port" type="port">
    <xs:annotation>
      <xs:documentation>
        The distributed protocol client socket connect internet port number.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="bind-port" type="port">
    <xs:annotation>
      <xs:documentation>
        The distributed protocol server socket bind internet port number.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="ssl-fips-enabled" type="boolean">
    <xs:annotation>
      <xs:documentation>
        Is ssl fips mode enabled for this host?
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="foreign-connect-port" type="port">
    <xs:annotation>
      <xs:documentation>
        The distributed protocol foreign client socket connect internet port number.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="foreign-bind-port" type="port">
    <xs:annotation>
      <xs:documentation>
        The distributed protocol foreign server socket bind internet port number.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="background-io-limit" type="xs:float">
    <xs:annotation>
      <xs:documentation>
        A rate limit for background I/O (MB/s, 0 means no limit).
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="host-name">
    <xs:annotation>
      <xs:documentation>
        The name of a Host
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[^ ].*"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="host-name" type="host-name">
    <xs:annotation>
      <xs:documentation>
        The internet host name.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="last-online" type="timestamp">
    <xs:annotation>
      <xs:documentation>
  	The time the host went online.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="last-offline" type="timestamp">
    <xs:annotation>
      <xs:documentation>
  	The time the host went offline.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="host">
    <xs:annotation>
      <xs:documentation>
        The host specification.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="host-id"/>
      <xs:element ref="host-name"/>
      <xs:element ref="connect-port"/>
      <xs:element ref="online"/>
      <xs:element ref="secure"/>
      <xs:element maxOccurs="unbounded" minOccurs="0" ref="last-online"/>
      <xs:element maxOccurs="unbounded" minOccurs="0" ref="last-offline"/>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="host" type="host">
    <xs:annotation>
      <xs:documentation>
        The host connection information.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="hosts">
    <xs:annotation>
      <xs:documentation>
        A set of hosts as seen by the host.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:choice maxOccurs="unbounded" minOccurs="0">
      <xs:element ref="host"/>
    </xs:choice>
  </xs:complexType>

  <xs:element name="hosts" type="hosts">
    <xs:annotation>
      <xs:documentation>
        The host's view of all other hosts in the cluster.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="transaction-mode">
    <xs:annotation>
      <xs:documentation>
        Transaction mode
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="query"/>
      <xs:enumeration value="update"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="transaction-mode" type="transaction-mode">
    <xs:annotation>
      <xs:documentation>
        The mode of a transaction
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="transaction-state">
    <xs:annotation>
      <xs:documentation>
        The state of a transaction
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="idle"/>
      <xs:enumeration value="active"/>
      <xs:enumeration value="awaiting commit"/>
      <xs:enumeration value="awaiting rollback"/>
      <xs:enumeration value="completed"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="transaction-state" type="transaction-state">
    <xs:annotation>
      <xs:documentation>
        The state of a transaction
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="transaction-id" type="xs:unsignedLong">
    <xs:annotation>
      <xs:documentation>
        The unique ID of a transaction.
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="server-id" type="xs:unsignedLong">
    <xs:annotation>
      <xs:documentation>
        The unique ID of an App Server (XDBC, ODBC, HTTP, WebDAV, or task server).
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="transaction-name" type="xs:string">
    <xs:annotation>
      <xs:documentation>
        The name of a transaction
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="transaction-timestamp" type="xs:unsignedLong">
    <xs:annotation>
      <xs:documentation>
        The timestamp of a transaction
      </xs:documentation>
      <xs:appinfo>
        <manage:units>posix-time</manage:units>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="database-id" type="xs:unsignedLong">
    <xs:annotation>
      <xs:documentation>
        The unique ID of the database.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="canceled" type="boolean">
    <xs:annotation>
      <xs:documentation>Has this transaction been canceled.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="timeout">
    <xs:annotation>
      <xs:documentation>An integer number of seconds, min 0, max 4294967295.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="4294967295"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="max-time-limit" type="timeout">
    <xs:annotation>
      <xs:documentation>The upper bound for a transaction's time limit, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>

  <xs:element name="join-size" type="xs:unsignedLong">
    <xs:annotation>
      <xs:documentation>An indication of the current size of the join tables created under this transaction.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>

  <xs:element name="time-limit" type="timeout">
    <xs:annotation>
      <xs:documentation>The transaction's time limit, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="user">
    <xs:annotation>
      <xs:documentation>A user ID.</xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>

  <xs:element name="user" type="user">
    <xs:annotation>
      <xs:documentation>The unique ID of the user executing the transaction.</xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="admin" type="xs:boolean">
    <xs:annotation>
      <xs:documentation>If the transaction has admin privileges.</xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="transaction">
    <xs:annotation>
      <xs:documentation>The status of a transaction</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="transaction-id"/>
      <xs:element ref="host-id"/>
      <xs:element ref="server-id"/>
      <xs:element maxOccurs="1" minOccurs="0" ref="xa:xid"/>
      <xs:element ref="transaction-name"/>
      <xs:element ref="transaction-mode"/>
      <xs:element ref="transaction-timestamp"/>
      <xs:element ref="transaction-state"/>
      <xs:choice>
        <xs:element ref="database-id"/>
        <xs:element maxOccurs="unbounded" ref="forest-id"/>
      </xs:choice>
      <xs:element ref="canceled"/>
      <xs:element ref="start-time"/>
