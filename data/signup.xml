  <script xml:space="preserve">
    $(document).ready(function() {

      var e = getParameterByName(&quot;e&quot;);
      if (e != &quot;&quot;) alert(e);

      $('#signup-form').validate({

        &quot;errorClass&quot;: &quot;signup-form-error&quot;,

        &quot;rules&quot;: {
          s_email: {
            maxlength: 255
          },
          s_password: {
            minlength: 6,
            maxlength: 255
          },
          s_password_confirm: {
            equalTo: &quot;#s_password&quot;
          }
        }
      });
      if (/[?&amp;]d=/.test(location.href)) {
        $(&quot;#signup-submit&quot;).text(&quot;Sign up and Download&quot;);
      }
    });

  </script>
