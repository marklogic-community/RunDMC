  <h1>Join the MarkLogic Community. It's free!</h1>

  <form method="post" action="/signup" id="signup-form" enctype="application/x-www-form-urlencoded">
    <span style="clear: both" id="signup-error"></span>
    <fieldset>
      <div class="signup-form-row" style="">
        <label class="signup-form-label" for="nothing"> </label>
        <div style="height:24px; font-size: 75%">Already a member? Just <a href="#" onclick="$('#login-menu').toggle();return false;" id="">log in</a>. Otherwise, all fields below are required.</div>
      </div>
      <div class="signup-form-row">
        <label class="signup-form-label" for="email">Email </label>
        <input class="required email" autofocus="autofocus" required="required" id="s_email" name="s_email" value="" type="text"/>
      </div>
      <div class="signup-form-row">
        <label class="signup-form-label" for="s_name">Full Name </label>
        <input class="required" required="required" id="s_name" name="s_name" value="" type="text"/>
      </div>
      <div class="signup-form-row">
        <label class="signup-form-label" for="s_password">Password</label>
        <input class="password required" required="required" id="s_password" name="s_password" value="" type="password"/>
        <!--
        <div class="password-meter" style="float: right">
            <div class="password-meter-message">&#160;</div>
            <div class="password-meter-bg">
                <div class="password-meter-bar"></div>
            </div>
        </div>
        -->
      </div>
      <div class="signup-form-row">
        <label class="signup-form-label" for="s_password_confirm">Confirm password</label>
        <input id="s_password_confirm" required="required" name="s_password_confirm" value="" type="password"/>
      </div>
      <div class="signup-form-row">
        <label class="signup-form-label" for="s_company">Phone</label>
        <input id="s_phone" required="required" name="s_phone" value="" type="text"/>
      </div>
