<!--
    Only static file paths that follow these rules will be accessible.
-->
<rules>
  <matches>^/(apidoc/)?js/.+\.js$</matches>
  <matches>^/(apidoc/)?images/.+\.(gif|ico|jpg|png|swf)$</matches>
  <starts-with>/controller/tracker.xqy</starts-with>
  <starts-with>/css/</starts-with>
  <starts-with>/download</starts-with>
  <starts-with>/favicon.ico</starts-with>
  <starts-with>/google978d12d96932d00a.html</starts-with>
  <starts-with>/hacks/</starts-with>
  <starts-with>/maven-license.html</starts-with>
  <starts-with>/robots.txt</starts-with>
  <starts-with>/stackunderflow/</starts-with>
  <starts-with>/test</starts-with>
  <starts-with>/try</starts-with>
</rules>
